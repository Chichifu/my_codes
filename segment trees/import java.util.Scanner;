import java.util.Scanner;
class practise1
{
	static int B[]=new int[50000];

	public static void main(String[]args)
	{
		Scanner scan=new Scanner(System.in)
		int t,i,j,k,l;
		t=scan.nextInt();
		int A[]=new int[t];
		for(i=0;i<t;i++)
		{
			A[i]=scan.nextInt();
		}
		build(1,0,n-1,A);
		for(j=0;j<2*t-1;j++)
		{
			System.out.println("tree["+j+"]"+"="+B[j]);
		}
	}

	void build(int node, int start, int end, intA[])
	{
		if(start==end)
		{
			B[node]=A[start];
		}
		else
		{
			int mid=(start+end)/2;

			build(2*node,start,mid,A);

			build(2*node+1,mid+1,end,A);

			B[node]=Math.amx(B[2*node],B[2*node+1]);
		}
	}
}